<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mddeloarhossain.bloodbank.FloatActionButton">

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/FloatActionButtonId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_margin="20dp"
        android:padding="20dp"
        android:src="@drawable/ic_add"

        />
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/FloatActionButton1Id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="90dp"
        android:layout_marginEnd="20dp"
        android:padding="20dp"
        android:visibility="invisible"
        android:src="@drawable/ic_call_black_24dp"

        />
    <android.support.design.widget.FloatingActionButton
    android:id="@+id/FloatActionButton2Id"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentBottom="true"
    android:layout_alignParentEnd="true"
    android:layout_marginRight="20dp"
    android:layout_marginBottom="160dp"
    android:layout_marginEnd="20dp"
    android:padding="20dp"
    android:visibility="invisible"
    android:src="@drawable/ic_message_black_24dp"

    />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/FloatActionButton3Id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="230dp"
        android:layout_marginEnd="20dp"
        android:padding="20dp"
        android:visibility="invisible"
        android:src="@drawable/ic_share_black_24dp"

        />
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/FloatActionButton4Id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="90dp"
        android:layout_marginBottom="20dp"
        android:layout_marginEnd="90dp"
        android:padding="20dp"
        android:visibility="invisible"
        android:src="@drawable/ic_delete_black_24dp"

        />
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/FloatActionButton5Id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="90dp"
        android:layout_marginBottom="90dp"
        android:layout_marginEnd="90dp"
        android:padding="20dp"
        android:visibility="invisible"
        android:src="@drawable/ic_edit_black_24dp"

        />
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/FloatActionButton6Id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="160dp"
        android:layout_marginBottom="160dp"
        android:layout_marginEnd="160dp"
        android:padding="20dp"
        android:visibility="invisible"
        android:src="@drawable/ic_record_voice_over_black_24dp"

        />
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/FloatActionButton7Id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="230dp"
        android:layout_marginBottom="230dp"
        android:layout_marginEnd="230dp"
        android:padding="20dp"
        android:visibility="invisible"
        android:src="@drawable/ic_attach_money_black_24dp"

        />


</RelativeLayout>
